{
  "name": "reactron",
  "version": "1.0.0",
  "description": "react-babel-webpack",
  "main": "main.js",
  "scripts": {
    "start": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "package": "yarn build && node_modules/.bin/electron-builder .",
    "dev": "webpack-dev-server --mode development --open --hot",
    "electron-dev": "concurrently \"webpack-dev-server --hot\" \"wait-on http://localhost:3001 && electron .\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "electron-builder install-app-deps",
    "electron": "electron ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.1",
    "css-loader": "^3.1.0",
    "electron": "^18.3.7",
    "electron-builder": "^21.1.5",
    "file-loader": "^4.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "react-hot-loader": "^4.12.10",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "wait-on": "^3.3.0",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "@skidding/react-codemirror": "^1.0.2",
    "bootstrap": "^4.3.1",
    "electron-reload": "^1.4.1",
    "jquery": "^3.4.1",
    "nodemon": "^1.19.1",
    "popper.js": "^1.15.0",
    "react": "^16.8.6",
    "react-desktop": "^0.3.9",
    "react-dom": "^16.8.6",
    "react-filetree-electron": "^1.2.2",
    "react-markdown": "^4.1.0",
    "react-split-pane": "^0.1.87"
  },
  "build": {
    "appId": "com.adikaplabs.reactron",
    "files": [
      "./**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  }
}
